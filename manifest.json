{
  "name": "Jízdní řády",
  "version": "1.0",
  "description": "Rychlé vyhledávání autobusových a vlakových spojů z adresního řádku.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "omnibox": {
    "keyword" : "idos"
  },
  "browser_action": {},
  "permissions": [
    "geolocation",
    "storage"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "web_accessible_resources": [
    "data/czech-cities.csv"
  ],
  "manifest_version": 2
}