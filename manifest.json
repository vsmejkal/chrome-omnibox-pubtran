{
  "name": "Jízdní řády",
  "version": "1.0",
  "description": "Vyhledávání autobusových a vlakových spojů přímo z adresního řádku.",
  "omnibox": {
    "keyword" : "idos"
  },
  "browser_action": {},
  "permissions": [
    "geolocation",
    "storage"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "web_accessible_resources": [
    "data/czech-cities.csv"
  ],
  "manifest_version": 2
}